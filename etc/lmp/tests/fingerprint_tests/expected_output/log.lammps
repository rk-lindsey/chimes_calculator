##############################################
### Variable definition - UPDATE as needed ###
##############################################

variable temp    equal 0	# Temperature (K)
variable seed  	 equal 1111	# Random seed
variable iofrq   equal 1	# Output frequency: thermo and trajectory MUST use same value
variable delt    equal 0.2	# Timestep (fs)
variable nstep   equal 6	# Number of steps for simulation


##############################################
### General settings - LEAVE EXACTLY AS IS ###
##############################################

units           real
newton          on
atom_style      atomic
atom_modify     sort 0 0.0

neighbor        1.0 bin
neigh_modify    delay 0 every 1 check yes

##############################################
###             Initialization             ###
##############################################

read_data       "HN3.2gcc_3000K.OUTCAR_#000.data.in"
Reading data file ...
  triclinic box = (0.0000000 0.0000000 0.0000000) to (16.500000 16.500000 16.500000) with tilt (0.0000000 0.0000000 0.0000000)
  1 by 1 by 1 MPI processor grid
  reading atoms ...
  512 atoms
  read_data CPU = 0.002 seconds
velocity        all create ${temp} ${seed} loop all mom yes rot yes dist gaussian
velocity        all create 0 ${seed} loop all mom yes rot yes dist gaussian
velocity        all create 0 1111 loop all mom yes rot yes dist gaussian

##############################################
###   Model definition - UPDATE as needed  ###
##############################################

pair_style	chimesFF fingerprint 2
pair_coeff	* * published_params.HN3.2+3+4b.Tersoff.special.offsets.txt

##############################################
###        Specify dyanmics                ###
##############################################

thermo_style    custom step time ke pe etotal temp press pxx pyy pzz pxy pxz pyz vol
thermo_modify   line one format float %20.5f flush yes
thermo          ${iofrq}
thermo          1

dump            1 all custom ${iofrq} traj.lammpstrj element xu yu zu fx fy fz
dump            1 all custom 1 traj.lammpstrj element xu yu zu fx fy fz
dump_modify     1 sort id element H N

timestep        ${delt}
timestep        0.2
run             ${nstep}
run             6
