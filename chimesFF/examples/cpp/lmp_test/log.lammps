##############################################
#
variable temp equal 1000

compute         ke_tensor all temp
thermo_style    custom step time ke pe etotal temp press pxx pyy pzz pxy pxz pyz vol c_ke_tensor[1] c_ke_tensor[2] c_ke_tensor[3] c_ke_tensor[4] c_ke_tensor[5] c_ke_tensor[6]
thermo_modify   line one format float %20.5f flush yes
thermo 1

timestep 0.1
dump            1 all custom 1 traj.lammpstrj id element xu yu zu fx fy fz
dump_modify 1 element C Si sort id
# Run
velocity all create ${temp} 2217 rot yes dist gaussian
velocity all create 1000 2217 rot yes dist gaussian
fix 2 all nvt temp ${temp} ${temp} 100
fix 2 all nvt temp 1000 ${temp} 100
fix 2 all nvt temp 1000 1000 100
fix_modify 2 energy yes
run 0
